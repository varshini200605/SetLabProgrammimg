        // try{
        //     //code that may throw an error
        // }catch(error){
        //     //code to handle the error
        // }finally{
        //     //code that will always execute
        // }  
        
        //example program
        //=== checks the type and content (datatypa and content)
        //== only checks the content
        //>0
        try{
            let atmpin=2345;
            if(atmpin===1234){
                throw "you entered wrong pin!."
            }
            console.log("you entered correct pin.")
        }
        catch(error){
            console.log(""+error)
        }
        finally{
            console.log("exection finished...")
        }

        //Definition
        //JSON.parse()-> converts a JSON string into a javascript object.
        //JSOn.stringify()-> converts a javascript object into a JSON string.
        let jsonstring='{"name":"java"}';
        let storedstring=JSON.parse(jsonstring);
        console.log(storedstring);

        let obj={name:"js"};
        let stredobj=JSON.stringify(obj);
        console.log(stredobj);

        fetch("data.json")
        .then(res=>res.json())
        .then(data=>{
            for(let students of data.student){
                console.log(students.name,"" ,students.id)
            }
        })
    </script>
